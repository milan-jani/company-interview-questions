class Solution {
  public:
    void left(Node *root,vector<int>& ans,int level){
      if(root==nullptr) return;
       if (level == ans.size()) {
            ans.push_back(root->data);
        }
     // ans.push_back(root->data);
      left(root->left,ans,level+1);
      left(root->right,ans,level+1);
      
  }
    vector<int> leftView(Node *root) {
        // code here
        vector<int> ans;
       left(root,ans,0);
       return ans;
        
        
    }
};
